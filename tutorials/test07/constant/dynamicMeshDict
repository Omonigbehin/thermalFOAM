dynamicFvMesh   dynamicRefineFvMesh;

dynamicRefineFvMeshCoeffs
{
    // All runtime refine/unrefine disabled; solver does the removal itself
    refineInterval  0;
    field           T;           // dummy
    lowerRefineLevel -1e30;
    upperRefineLevel  1e30;
    unrefineLevel    -1e30;
    nBufferLayers     1;
    maxRefinement     0;
    maxCells          2000000000;
    correctFluxes     ( );
    dumpLevel         false;
}
