FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         backward;
    
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default         none;
    
}

laplacianSchemes
{
    default         none;
    
    laplacian(Kth,T) Gauss harmonic corrected;
    
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}

